{
  "expo": {
    "name": "HackerBuzz",
    "slug": "HackerBuzz",
    "owner": "Ajay Sharma",
    "contact": "ajshrmaofficial@gmail.com",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/hacker-news.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.HackerBuzz"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/hacker-news.jpg",
        "backgroundColor": "#000"
      },
      "googleServicesFile": "./android/app/google-services.json",
      "package": "com.anonymous.HackerBuzz"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/hacker-news.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/GloriaHallelujah.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
